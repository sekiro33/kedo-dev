   justify-content: center;\n            font-size: .75rem;\n            line-height: 1rem;\n            font-weight: 700;\n        }\n        .vanilla-calendar-months__month, .vanilla-calendar-years__year {\n            display: flex;\n            height: 2.5rem;\n            align-items: center;\n            justify-content: center;\n            word-break: break-all;\n            border-radius: .5rem;\n            border-width: 0px;\n            padding: .25rem;\n            text-align: center;\n            font-size: .75rem;\n            line-height: 1rem;\n            font-weight: 600;\n        }\n        .vanilla-calendar-months__month, .vanilla-calendar-years__year {\n            display: flex;\n            height: 2.5rem;\n            align-items: center;\n            justify-content: center;\n            word-break: break-all;\n            border-radius: .5rem;\n            border-width: 0px;\n            padding: .25rem;\n            text-align: center;\n            font-size: .75rem;\n            line-height: 1rem;\n            font-weight: 600;\n        }\n        .content_paginator{\n            display:flex;\n            align-items:center;\n            font-size:18px;\n            font-weight:600;\n            font-style:normal;\n            line-height:22px;\n            padding:0 .5rem;\n            max-height: unset;\n        }\n        .paginator_item{\n            color:#05f;\n            padding:.75rem 1rem;\n            transition:all .3s;\n            border-radius:4px\n        }\n        .paginator_item:hover{\n            cursor:pointer;\n            background-color:#e3ebfc\n        }\n        .paginator_item.active{\n            color:#fff;\n            background-color:#5082e6\n        }\n        .paginator-item::marker {\n            content: \"\";\n        }\n        .paginationjs-ellipsis::marker,\n        .paginationjs-ellipsis.disabled::marker {\n            content: \"\"\n        }\n        .paginator-button{\n            color:#05f\n        }\n        .paginator-button:hover{\n            cursor:pointer\n        }\n        .search-item{\n            padding:.5rem 1rem;\n            cursor:pointer\n        }\n        .search-item:hover{\n            background-color:#f3f6ff\n        }\n        .tasks-page_main-content_title_search-extend_item .title-search{\n            border:1px solid #d9d9d9\n        }\n        .common-widget-title{\n            display:flex;\n            justify-content:space-between;\n            gap:1rem\n        }\n        .common-content_title_text{\n            pointer-events:none\n        }\n        .common-content_table{\n            display:flex;\n            flex-direction:column;\n            width:100%;\n            border-radius:8px;\n            background-color:#fff;\n            font-size:15px;\n            font-weight:400\n        }\n        .common-content_table.hidden{\n            display:none\n        }\n        .common-content_table-item{\n            display:flex;\n            flex-grow:1;\n            padding:1rem 2rem;\n            gap:1rem;\n            align-items:center\n        }\n        .common-table-content{\n            display:flex;\n            flex-direction:column;\n            width:100%;\n            padding:.5rem 0\n        }\n        .common-table-content .common-content_table-item{\n            padding:1rem 1.5rem;\n            margin:0 .5rem;\n            border-radius:8px;\n            min-height:5.75rem\n        }\n        .common-table-content .common-content_table-item:hover{\n            background-color:#f3f6ff;\n            cursor:pointer\n        }\n        .common-content_title_search-extend{\n            display:flex;\n            flex-direction:column;\n            padding:1rem 1.5rem;\n            position:absolute;\n            width:100%;\n            box-sizing:border-box;\n            height:fit-content;\n            background-color:#fff;\n            box-shadow:0 6px 28px 0 rgba(37,41,65,.16);\n            top:3.5