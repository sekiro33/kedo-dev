msgid "\u003cstyle\u003e\n  .spinner-svg {\n    animation: spin 1s linear infinite;\n    position: absolute;\n    right: -5rem;\n  }\n  @keyframes spin {\n    from {\n      transform: rotate(0deg);\n    }\n    to {\n      transform: rotate(360deg);\n    }\n  }\n  .trash-icon-container {\n    position: absolute;\n    right: -3.5rem;\n  }\n  .custom-trash-icon {\n    cursor: pointer;\n  }\n  .kedo-table {\n      border-collapse: separate;\n      border-spacing: 3rem;\n      white-space: normal;\n      width: 100%;\n  }\n  .inputfile {\n      width: 0.1px;\n      height: 0.1px;\n      opacity: 0;\n      overflow: hidden;\n      position: absolute;\n      z-index: -1;\n  }\n  .custom-label {\n      display: inline-block;\n      margin-bottom: 0;\n      color: #787878;\n      font-size: 16px;\n      transition: all 0.5s;\n  }\n\n  .custom-label:hover {\n      cursor: pointer;\n      color: #5082E6;\n  }\n\n  .doc-row {\n      display: flex;\n      align-items: center;\n      border-bottom: 1px solid #D9D9D9;\n      padding: 1rem;\n      justify-content: space-between;\n      position: relative;\n  }\n\n  .doc-row.required {\n    color: black;\n  }\n  .doc-row.required::after {\n    content: \"*\";\n    color: #F5222D;\n    right: 0rem;\n    position: absolute;\n  }\n\n  .doc-file {\n      display: flex;\n      align-items: center;\n      gap: 1rem;\n      position: relative;\n  }\n\n  .input-blocker {\n    position: absolute;\n    width: 100%;\n    height: 100%;\n    z-index: 1000;\n  }\n\n  .input-blocker.hidden {\n    display: none;\n  }\n\n  .kedo__step3-content-btn-cancel-wrapper {\n    position: relative;\n  }\n  .button-add-element {\n    display: none;\n  }\n  .big-file-warning {\n    position: absolute;\n    color: red;\n    left: 27rem;\n    font-size: 13px;\n    top: 0rem;\n    opacity: 1;\n    transition: all 0.5s;\n  }\n  .big-file-warning.hidden {\n    transition: all 0.5s;\n    top: -1rem;\n    opacity: 0;\n  }\n  .kedo-form__wizard {\n    list-style: none;