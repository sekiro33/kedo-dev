msgctxt ".descriptor.template.content.[4].values.html"
msgid "\u003cstyle\u003e\n.simple-notification-wrapper {\n    display: none !important;\n}\n.kedo-form__form app-collection-readonly-link {\n    pointer-events: none;\n    overflow: unset;\n}\nelma-table-full-field {\n    display: flex !important;\n    align-items: center !important;\n}\nelma-type-file.required::before {\n    content: \"*\";\n    color: red;\n    position: absolute;\n    top: 0;\n}\nelma-type-table-full-line tr td {\n    border: none !important;\n    border-bottom: 1px solid #D9D9D9 !important;\n}\ntable elma-type-table-full-line:nth-last-child(-n + 2) {\n    display: none;\n}\n\ntable elma-type-table-full-line:first-child {\n    display: none;\n}\n.drop-area-wrapper button {\n    display: none;\n}\n\n/********Date fix********/\n\n.ui-datepicker-trigger {\n    pointer-events: none;\n    display: none !important;\n}\n\n#date_of_birth, #date_of_issue {\n    width: 75%;\n    padding-right: 0\n}\n\n@media and (min-width: 1024px) {\n    .elma-form-row.\\@compact {\n        flex: 1 0 100% !important;\n        margin-bottom: 7px !important;\n    }\n\n    .elma-form-row.\\@compact .elma-form-label {\n        flex: 0 0 calc(146px + 2em) !important;\n        max-width: calc(146px + 2em) !important;\n        padding: 1rem 7px 1rem 0 !important;\n        margin-right: 14px !important;\n        font-size: 1.5rem !important; \n    }\n\n    .elma-form-row.\\@compact .elma-form-control {\n        flex-basis: 0 !important;\n        flex-grow: 1 !important;\n        min-width: 160px !important;\n    }\n}\n\u003c/style\u003e"
msgstr "\u003cstyle\u003e\n.simple-notification-wrapper {\n    display: none !important;\n}\n.kedo-form__form app-collection-readonly-link {\n    pointer-events: none;\n    overflow: unset;\n}\nelma-table-full-field {\n    display: flex !important;\n    align-items: center !important;\n}\nelma-type-file.required::before {\n    content: \"*\";\n    color: red;\n    position: absolute;\n    top: 0;\n}\nelma-type-table-full-line tr td {\n    border: none !important;\n    border-bottom: 1px solid #D9D9D9 !important;\n}\ntable elma-type-table-full-line:nth-last-child(-n + 2) {\n    display: none;\n}\n\ntable elma-type-table-full-line:first-child {\n    display: none;\n}\n.drop-area-wrapper button {\n    display: none;\n}\n\n/********Date fix********/\n\n.ui-datepicker-trigger {\n    pointer-events: none;\n    display: none !important;\n}\n\n#date_of_birth, #date_of_issue {\n    width: 75%;\n    padding-right: 0\n}\n\n@media and (min-width: 1024px) {\n    .elma-form-row.\\@compact {\n        flex: 1 0 100% !important;\n        margin-bottom: 7px !important;\n    }\n\n    .elma-form-row.\\@compact .elma-form-label {\n        flex: 0 0 calc(146px + 2em) !important;\n        max-width: calc(146px + 2em) !important;\n        padding: 1rem 7px 1rem 0 !important;\n        margin-right: 14px !important;\n        font-size: 1.5rem !important; \n    }\n\n    .elma-form-row.\\@compact .elma-form-control {\n        flex-basis: 0 !important;\n        flex-grow: 1 !important;\n        min-width: 160px !important;\n    }\n}\n\u003c/style\u003e"

