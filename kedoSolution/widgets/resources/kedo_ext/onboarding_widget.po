msgctxt ".descriptor.template.content.[5].values.html"
msgid "\n\u003cdiv class=\"ui-teml\" style=\"display: none;width: 900px\"\u003e\n        \u003cdiv id=\"step-1-0\"\u003e\n                \u003c%= UI.widget.contextRow(\"surname\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-0\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"name\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-0\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"patronymic\", {required: false, onChange: () =\u003e Scripts.validate(\"step-1-0\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"date_of_birth\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-0\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"gender\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-0\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"email_work\", {required: true, readonly: true, onChange: () =\u003e Scripts.validate(\"step-1-0\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"phone_number_work\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-0\", enableNextButton)}) %\u003e\n        \u003c/div\u003e\n        \u003cdiv id=\"step-1-1\"\u003e\n                \u003c%= UI.widget.contextRow(\"region_app\", {required: false, onChange: () =\u003e Scripts.validate(\"step-1-1\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"city\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-1\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"street\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-1\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"house\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-1\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"housing\", {required: false}) %\u003e\n                \u003c%= UI.widget.contextRow(\"flat\", {required: false}) %\u003e\n        \u003c/div\u003e\n        \u003cdiv id=\"step-1-2\"\u003e\n                \u003cp class=\"kedo-form__group-title\"\u003eПаспорт\u003c/p\u003e\n                \u003c%= UI.widget.contextRow(\"russian_passport\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-2\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"passport_series\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-2\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"passport_number\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-2\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"date_of_issue\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-2\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"issuer\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-2\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"issuer_code\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-2\", enableNextButton)}) %\u003e\n                \u003cp class=\"kedo-form__group-title\"\u003eДокументы\u003c/p\u003e\n                \u003c%= UI.widget.contextRow(\"inn\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-2\", enableNextButton)}) %\u003e\n                \u003c%= UI.widget.contextRow(\"snils\", {required: true, onChange: () =\u003e Scripts.validate(\"step-1-2\", enableNextButton)}) %\u003e\n        \u003c/div\u003e\n        \u003cdiv id=\"step-1-3\"\u003e\n                \u003ctable class=\"kedo-table\"\u003e\n                        \n                \u003c/table\u003e\n                \u003c!-- \u003c%= UI.widget.contextRow(\"doc_table\", {name: \" \", required: false, onChange: () =\u003e Scripts.validate(\"step-1-3\", enableNextButton)}