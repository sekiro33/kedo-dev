or: var(--text-gray);\n    text-align: center;\n    width: 80%;\n\n    margin-bottom: 40px;\n  }\n\n  .kedo-form__step-container {\n    position: relative;\n  }\n\n  .kedo-form__step-name {\n    color: var(--text-light-gray);\n    font-size: 12px;\n    line-height: 130%;\n    text-align: center;\n    word-wrap: break-word;\n\n    position: absolute;\n    left: 50%;\n    transform: translateX(-50%);\n    margin: 12px 0;\n    width: 150px;\n  }\n\n  .kedo-form__step-container--active .kedo-form__step-name {\n    color: var(--text-gray);\n  }\n\n  .kedo-form__fields {\n    width: 100%;\n    padding: 0 40px 44px 56px;\n  }\n\n  .kedo-form__controls {\n  position: relative;\n  display: flex;\n  flex: 1 0 100%;\n  flex-wrap: wrap;\n  width: 100%;\n  min-width: 100%;\n  margin: 25px 0 7px 0;\n}\n\n.kedo-form__pseudo-controls-element {\n  line-height: 1.1;\n  flex: 0 0 calc(146px + 2em);\n  max-width: calc(146px + 2em);\n  padding: 1rem 7px 1rem 0;\n  margin-right: 14px;\n}\n\n.kedo-form__next-btn {\n  border-radius: 4px;\n  transition: 0.3s;\n  font-weight: 600;\n  font-size: 17px;\n  line-height: 24px;\n\n  background: var(--kedo-main-color);\n  border: 1px solid var(--kedo-main-color);\n  color: #FFFFFF;\n  padding: 8px 16px;\n}\n\n.kedo-form__next-btn:hover {\n  cursor: pointer;\n  background: var(--kedo-main-color-hover);\n  border: 1px solid var(--kedo-main-color-hover);\n  color: #FFFFFF;\n  text-decoration: none;\n}\n\n.kedo-form__next-btn[disabled] {\n  background: #aaaaaa;\n  border: 1px solid #aaa;\n  cursor: default;\n}\n\n.kedo-form__prev-btn {\n  border: none;\n  background: none;\n  position: relative;\n\n  color: var(--kedo-main-color);\n  font-size: 15px;\n  line-height: 32px;\n\n  margin-left: 16px;\n  padding-left: 0;\n  cursor: pointer;\n}\n\n.kedo-form__prev-btn {\n  color: var(--color-theme-400)\n}\n\n.kedo-form__prev-btn::before {\n  content: \"\";\n\n  border: solid var(--kedo-main-color);\n  border-width: 0 2px 2px 0;\n  display: inline-block;\n  pa