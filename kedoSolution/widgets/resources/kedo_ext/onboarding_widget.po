msgid "\n\n  \u003cdiv class=\"kedo\"\u003e\n    \u003cdiv class=\"unep-modal-backdrop\"\u003e\n      \n    \u003c/div\u003e\n    \u003cdiv class=\"kedo__page\"\u003e\n      \u003cheader class=\"kedo__header\"\u003e\n        \u003cdiv class=\"kedo__header-content content\"\u003e\n          \u003cdiv class=\"kedo__header-row\"\u003e\n            \u003cdiv class=\"kedo__header-logo-wrapper\"\u003e\n              \u003c!-- \u003cp class=\"kedo__header-logo-text\"\u003eЛоготип\u003c/p\u003e --\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"kedo__header-nav\"\u003e\n              \u003cdiv class=\"kedo__header-user-info-wrapper\"\u003e\n                \u003cdiv class=\"kedo__header-user-info-img\"\u003e\u003c/div\u003e\n                \u003cdiv class=\"kedo__header-user-info-text-wrapper\"\u003e\n                  \u003cp class=\"kedo__header-user-info-name\"\u003e\u003c/p\u003e\n                  \u003cp class=\"kedo__header-user-info-firm\"\u003e\u003c/p\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n              \u003cbutton class=\"kedo__header-btn-exit text-underline\"\u003eВыйти\u003c/button\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/header\u003e\n\n      \u003cdiv class=\"kedo__content content\"\u003e\n        \u003cdiv class=\"kedo__content-row\"\u003e\n          \u003cdiv class=\"kedo__content-column kedo__content-column_main\"\u003e\n          \u003c/div\u003e\n\n          \u003cdiv class=\"kedo__content-column kedo__content-column_right\"\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n\n      \u003cfooter class=\"kedo__footer footer\"\u003e\n        \u003cp class=\"footer__text\"\u003e\u003c%- textFooterPortal %\u003e\u003c/p\u003e\n      \u003c/footer\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \n\n  \u003cdiv class=\"kedo-loader-wrapper\"\u003e\n    \u003cdiv class=\"kedo-loader-img\"\u003e\u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cdiv class=\"kedo-popap-personal-data\"\u003e\n    \u003cdiv class=\"kedo-popap-personal-data__container\"\u003e\n      \u003c!-- \u003cbutton class=\"kedo-popap-personal-data__btn-close\"\u003e\u003c/button\u003e --\u003e\n      \u003ch3 class=\"kedo-popap-personal-data__title\"\u003eСогласие на обработку персональных данных\u003c/h3\u003e\n      \u003cp class=\"kedo-popap-personal-data__text\"\u003eДля дальнейшего взаимодействия \u003cspan class=\"kedo-popap-personal-data__text-firm\"\u003e\u003c/span\u003e на портале КЭДО необходимо дать\n          согласие на обработку персональных данных и ознакомиться с \u003ca class=\"license-link\"\u003eлицензионным соглашением\u003c/a\u003e.\u003c/p\u003e\n      \u003clabel for=\"\" class=\"kedo-popap-personal-data__input-label\"\u003e\n          \u003cinput type=\"checkbox\" name=\"\" class=\"kedo-popap-personal-data__input\"\u003e\n          Я даю \u003cspan class=\"kedo-popap-personal-data__input-approval\"\u003eсогласие\u003c/span\u003e на обработку персональных данных в соответствии \u003ca href=\"#\" class=\"kedo-popap-personal-data__input-label-link\"\u003eс политикой конфиденциальности\u003c/a\u003e.\n      \u003c/label\u003e\n      \u003cbutton disabled class=\"kedo-popap-personal-data__button kedo-button kedo-button_color_blue\"\u003eОтправить\u003c/button\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003ctemplate class=\"file_line-template\"\u003e\n    \u003cdiv class=\"file_line-file common-file-field kedo-hidden\"\u003e\n        \u003csvg class=\"file-svg\" width=\"20\" height=\"20\" viewBox=\"0 0 20 20\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\"\u003e\n            \u003cpath d=\"M4.16406 15.8333V18.3333H15.8307V15.8333\" stroke=\"#5082E6\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/\u003e\n            \u003cpath d=\"M15.8307 8.33332V5.83332L12.4974 1.66666H4.16406V8.33332\" stroke=\"#5082E6\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/\u003e\n            \u003cpath d=\"M11.6641 1.66666V5.83332H15.8307\" stroke=\"#5082E6\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/\u003e\n            \u003cpath d=\"M17.4974 8.33334H2.4974C2.03716 8.33334 1.66406 8.70644 1.66406 9.16668V15C1.66406 15.4602 2.03716 15.8333 2.4974 15.8333H17.4974C17.9576 15.8333 18.3307 15.4602 18.3307 15V9.16668C18.3307 8.70644 17.9576 8.33334 17.4974 8.33334Z\" stroke=\"#5082E6\" stroke-linejoin=\"round\"/\u003e\n            \u003cpath d=\"M8.75 10.4167V13.75\" stroke=\"#5082E6\" stroke-linecap=\"round\"/\u003e\n            \u003cpath d=\"M4.16406 10.4167V13.75\" stroke=\"#5082E6\" stroke-linecap=\"round\"/\u003e\n            \u003cpath d=\"M13.334 13.75V10.4167H15.4173\" stroke=\"#5082E6\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/\u003e\n            \u003cpath d=\"M13.334 12.5H15.4173\" stroke=\"#5082E6\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/\u003e\n            \u003cpath d=\"M4.16406 10.4167H5.6224C6.19769 10.4167 6.66406 10.883 6.66406 11.4583C6.66406 12.0336 6.19769 12.5 5.6224 12.5H4.16406\" stroke=\"#5082E6\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/\u003e\n            \u003cpath d=\"M8.75 10.4167H9.58333C10.5038 10.4167 11.25 11.1629 11.25 12.0833C11.25 13.0038 10.5038 13.75 9.58333 13.75H8.75\" stroke=\"#5082E6\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/\u003e\n            \u003cpath d=\"M6.66406 5H8.33073\" stroke=\"#5082E6\" stroke-linecap=\"round\"/\u003e\n        \u003c/svg\u003e\n        \u003ca class=\"file_line-file_name common-link\"\u003e\n            \n        \u003c/a\u003e\n    \u003c/div\u003e\n\u003c/template\u003e\n\n  \u003ctemplate class=\"unep_goskey_footer-template\"\u003e\n    \u003cdiv class=\"unep_goskey_footer\"\u003e\n      \u003cbutton class=\"unep_goskey_footer-button\"\u003eУведомить об авторизации\u003c/button\u003e\n    \u003c/div\u003e\n  \u003c/template\u003e\n\n  \u003ctemplate class=\"step3_goskey_footer-template\"\u003e\n    \u003cdiv class=\"unep_goskey_footer\"\u003e\n      \u003cdiv class=\"step3-goskey-reject hidden\"\u003e\n        \u003cp\u003eВ случае отказа оставьте комментарий\u003c/p\u003e\n        \u003ctextarea class=\"step3-goskey-reject_comment\"\u003e\u003c/textarea\u003e\n        \u003cdiv class=\"goskey-reject-buttons\"\u003e\n          \u003cbutton id=\"goskey-send-comment\" class=\"unep_goskey_footer-button\"\u003eОтправить\u003c/button\u003e\n          \u003cbutton id=\"goskey-hide-comment\" class=\"unep_goskey_footer-button danger\"\u003eОтмена\u003c/button\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cbutton id=\"goskey-signed\" class=\"unep_goskey_footer-button disabled\"\u003eОзнакомился и подписал\u003c/button\u003e\n      \u003cbutton id=\"goskey-reject-popup\" class=\"unep_goskey_footer-button common\"\u003eТребуется уточнение\u003c/button\u003e\n    \u003c/div\u003e\n  \u003c/template\u003e\n  \n  \u003ctemplate class=\"unep_choice_footer-template\"\u003e\n    \u003cdiv class=\"unep_choice_footer\"\u003e\n      \u003cdiv class=\"unep_choice_footer-choice_container\"\u003e\n        \n      \u003c/div\u003e\n      \u003cbutton class=\"unep_goskey_footer-button disabled\"\u003e\n        Уведомить о выборе\n      \u003c/button\u003e\n    \u003c/div\u003e\n  \u003c/template\u003e\n\n\u003ctemplate class=\"choice_radio-container-template\"\u003e\n  \u003cdiv class=\"choice_radio-container\"\u003e\n    \u003cinput class=\"unep_radio\" type=\"radio\" id=\"\" name=\"\" value=\"\" /\u003e\n    \u003clabel for=\"\"\u003e\u003c/label\u003e\n  \u003c/div\u003e\n\u003c/template\u003e\n\n  \u003ctemplate class=\"kedo-error-wrapper-template\"\u003e\n    \u003cdiv class=\"kedo-error-wrapper\"\u003e\n      \u003ch2 class=\"kedo-error-text\"\u003e\u003c/h2\u003e\n    \u003c/div\u003e\n  \u003c/template\u003e\n\n  \u003ctemplate class=\"kedo__window-wrapper-template\"\u003e\n    \u003csection class=\"kedo__window-wrapper\"\u003e\n      \u003cdiv class=\"kedo__window-container\"\u003e\n        \u003cdiv class=\"kedo__window-header\"\u003e\n          \u003cdiv class=\"kedo__window-header-nav\"\u003e\n            \u003cul class=\"kedo__window-header-items\"\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item_active\"\u003e\n                \u003cp class=\"kedo__window-header-item-text kedo__window-header-item-text--first\"\u003e1\u003c/p\u003e\n                \u003cp class=\"kedo__window-header-item-text\"\u003eЗаполнение данных\u003c/p\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item--sub\"\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item\"\u003e\n                \u003cp class=\"kedo__window-header-item-text kedo__window-header-item-text--first\"\u003e2\u003c/p\u003e\n                \u003cp class=\"kedo__window-header-item-text\"\u003eПодписание соглашения\u003c/p\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item--sub\"\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item\"\u003e\n                \u003cp class=\"kedo__window-header-item-text kedo__window-header-item-text--first\"\u003e3\u003c/p\u003e\n                \u003cp class=\"kedo__window-header-item-text\"\u003eВыпуск ЭП\u003c/p\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item--sub\"\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item\"\u003e\n                \u003cp class=\"kedo__window-header-item-text kedo__window-header-item-text--first\"\u003e4\u003c/p\u003e\n                \u003cp class=\"kedo__window-header-item-text\"\u003eПодписание документов\u003c/p\u003e\n              \u003c/li\u003e\n            \u003c/ul\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"kedo__window-content\"\u003e\n          \u003c!-- todo Димин код --\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \n      \u003cfooter class=\"kedo__content-footer footer\"\u003e\n        \u003cp class=\"footer__text\"\u003e\u003c%- textFooterPortal %\u003e\u003c/p\u003e\n      \u003c/footer\u003e\n    \u003c/section\u003e\n  \u003c/template\u003e\n\n  \u003ctemplate class=\"kedo__on-check-template\"\u003e\n    \u003csection class=\"kedo__on-check-wrapper\"\u003e\n      \u003cdiv class=\"kedo__on-check-content-container\"\u003e\n        \u003cdiv class=\"kedo__window-header\"\u003e\n          \u003cdiv class=\"kedo__window-header-nav\"\u003e\n            \u003cul class=\"kedo__window-header-items\"\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item_active\"\u003e\n                \u003cp class=\"kedo__window-header-item-text kedo__window-header-item-text--first\"\u003e1\u003c/p\u003e\n                \u003cp class=\"kedo__window-header-item-text\"\u003eЗаполнение данных\u003c/p\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item--sub\"\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item\"\u003e\n                \u003cp class=\"kedo__window-header-item-text kedo__window-header-item-text--first\"\u003e2\u003c/p\u003e\n                \u003cp class=\"kedo__window-header-item-text\"\u003eПодписание соглашения\u003c/p\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item--sub\"\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item\"\u003e\n                \u003cp class=\"kedo__window-header-item-text kedo__window-header-item-text--first\"\u003e3\u003c/p\u003e\n                \u003cp class=\"kedo__window-header-item-text\"\u003eВыпуск ЭП\u003c/p\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item--sub\"\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item\"\u003e\n                \u003cp class=\"kedo__window-header-item-text kedo__window-header-item-text--first\"\u003e4\u003c/p\u003e\n                \u003cp class=\"kedo__window-header-item-text\"\u003eПодписание документов\u003c/p\u003e\n              \u003c/li\u003e\n            \u003c/ul\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"kedo__on-check-container\"\u003e\n          \u003cdiv class=\"kedo__on-check-img\"\u003e\u003c/div\u003e\n          \u003ch3 class=\"kedo__on-check-title\"\u003e\u003c/h3\u003e\n          \u003cp class=\"kedo__on-check-text\"\u003e\u003c/p\u003e\n        \u003c/div\u003e\n    \n        \u003cfooter class=\"kedo__content-footer footer\"\u003e\n          \u003cp class=\"footer__text\"\u003e\u003c%- textFooterPortal %\u003e\u003c/p\u003e\n        \u003c/footer\u003e\n      \u003c/div\u003e\n    \u003c/section\u003e\n  \u003c/template\u003e\n\n  \u003ctemplate class=\"kedo__unep-template\"\u003e\n    \u003csection class=\"kedo__on-check-wrapper\"\u003e\n      \u003cdiv class=\"kedo__on-check-content-container\"\u003e\n        \u003cdiv class=\"kedo__window-header\"\u003e\n          \u003cdiv class=\"kedo__window-header-nav\"\u003e\n            \u003cul class=\"kedo__window-header-items\"\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item_active\"\u003e\n                \u003cp class=\"kedo__window-header-item-text kedo__window-header-item-text--first\"\u003e1\u003c/p\u003e\n                \u003cp class=\"kedo__window-header-item-text\"\u003eЗаполнение данных\u003c/p\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item kedo__window-header-item--sub\"\u003e\n              \u003c/li\u003e\n              \u003cli class=\"kedo__window-header-item\"\u003e\n                \u003cp class=\