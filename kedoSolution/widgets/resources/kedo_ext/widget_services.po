msgid "\u003cstyle\u003e\n    .services-page_main-content_all-services {\n        display: flex;\n        flex-wrap: wrap;\n        background-color: white;\n        border-radius: 8px;\n        padding: 1.5rem;\n        flex-direction: column;\n        justify-content: flex-start;\n        max-height: 75rem;\n        row-gap: 2rem;\n    }\n\n    .services-page_main-content_all-services .favorite-services_choice_column {\n        display: flex;\n        flex-direction: column;\n        //flex-basis: 20rem;\n        //gap: 2rem;\n        width: 30%;\n    }\n\n    .services-page_main-content_all-services .favorite-services_choice_column-title {\n        font-size: 16px;\n    }\n\n    .services-page_main-content_all-services .favorite-services_choice_column .new-issue_container_item-list {\n        padding: .5rem 0;\n    }\n\n    .services-page_main-content_all-services .favorite-services_choice_column .new-issue_container_item-list:hover {\n        background-color: #FFF;\n    }\n\n    .services-page_main-content_all-services .new-issue_container_item-list {\n        background-color: #FFF;\n        font-size: 16px;\n        font-style: normal;\n        font-weight: 400;\n        padding: 0.5rem 1rem;\n        display: flex;\n        align-items: center;\n        justify-content: start;\n        gap: .5rem;\n        transition: background-color 0.3s;\n    }\n\n    .services-page_main-content_all-services a.new-issue_container_item-list {\n        text-decoration: none;\n        color: black;\n    }\n\n    .services-page_main-content_all-services .new-issue_container_item-list:hover {\n        background-color: #f5f6fa;\n        cursor: pointer;\n        user-select: none;\n    }\n\n    .services-page_main-content_all-services .favorite-services_choice_column .new-issue_container_item-list:hover .new-issue_container_item_list-icon path {\n        background-color: #CBDCFF !important;\n    }\n\n    .services-page_main-content_all-services .new-issue_container_item-list:hover .new-issue_container_item_list-icon {\n        background-color: #FFF !important;\n    }\n\n    .services-page_main-content_all-services .new-issue_container_item_list-icon path {\n        stroke: #0055FF;\n        stroke-width: 1.2px;\n        fill: none;\n    }\n\n    .services-page_main-content_all-services .new-issue_container_item_list-icon.path-svg path {\n        stroke: #0055FF;\n        stroke-width: 1.2px;\n        fill: none;\n    }\n\n    .services-page_main-content_all-services .new-issue_container_item_list-icon {\n        border-radius: 50%;\n        background-color: #CBDCFF;\n        width: 32px;\n        height: 32px;\n        min-width: 32px;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n    }\n\n    /*.services-page_main-content_all-services .new-issue_container_item_list-label-text {\n        white-space: nowrap;\n    }*/\n\n    /*@media (max-width: 1200px) {\n        .services-page_main-content_all-services {\n            flex-direction: row;\n            max-height: fit-content;\n        }\n    }*/\n\n    @media (max-width: 900px) {\n        .services-page_main-content_all-services .favorite-services_choice_column {\n            gap: 1rem;\n        }\n\n        .services-page_main-content_all-services .favorite-services_choice_column .new-issue_container_item-list {\n            max-width: unset;\n        }\n    }\n\n    @media (max-width: 450px) {\n        .services-page_main-content_all-services {\n            flex-direction: row;\n            max-height: fit-content;\n        }\n\n        .services-page_main-content_all-services .favorite-services_choice_column {\n            gap: 1rem;\n            width: 100%;\n        }\n\u003c/style\u003e\n\n\u003ctemplate class=\"favorite-services_choice_column_template\"\u003e\n    \u003cdiv class=\"favorite-services_choice_column\"\u003e\n        \u003ch4 class=\"favorite-services_choice_column-title\"\u003e\n\n        \u003c/h4\u003e\n    \u003c/div\u003e\n\u003c/template\u003e\n\n\u003ctemplate class=\"new-issue_container_item-list-template\"\u003e\n    \u003ca class=\"new-issue_container_item-list\"\u003e\n        \u003cdiv class=\"new-issue_container_item_list-icon\"\u003e\n\n        \u003c/div\u003e\n        \u003cdiv class=\"new-issue_container_item_list-label\"\u003e\n            \u003cp class=\"new-issue_container_item_list-label-text\"\u003e\n\n            \u003c/p\u003e\n        \u003c/div\u003e\n    \u003c/a\u003e\n\u003c/template\u003e\n\n\u003cdiv class=\"services-page_main-content_all-services\"\u003e\n\n\u003c/div\u003e"
