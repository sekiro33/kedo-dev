msgctxt ".descriptor.template.content.[1].content.[1].content.[2].values.html"
msgid "\u003cstyle\u003e\n    * {\n        margin: 0;\n        padding: 0;\n        font-family: 'SF Pro Display';\n        margin-top: 0;\n        margin-bottom: 0;\n        padding-top: 0;\n        padding-bottom: 0;\n        font-size: 16px;\n    }\n\n    .--elma-desktop-mode .content-body__inner {\n        padding: 0;\n    }\n\n    .unemployed {\n        position: absolute;\n        width: 100%;\n        height: 100%;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        z-index: 100;\n        background-color: #FFF;\n    }\n\n    .unemployed.hidden {\n        display: none;\n    }\n\n    .settings_modal-container {\n        position: absolute;\n        width: 100%;\n        height: 100%;\n        background-color: #00000033;\n        z-index: 100;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        transition: all .3s;\n        opacity: 1;\n    }\n\n    .settings_modal-container.hidden {\n        opacity: 0;\n        z-index: -1;\n    }\n\n    .settings_modal {\n        display: flex;\n        flex-direction: column;\n        padding: 2rem 0;\n        gap: 1rem;\n        width: min-content;\n        min-width: 40%;\n        max-width: 100%;\n        background-color: #FFF;\n    }\n\n    .settings_modal-title {\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n        padding: 0 2rem;\n    }\n\n    .settings_modal-title_close {\n        color: #BFBFBF;\n        transition: color .3s;\n        user-select: none;\n    }\n\n    .settings_modal-title_close:hover {\n        color: #0055FF;\n        cursor: pointer;\n    }\n\n    .settings_modal-main {\n        padding: 2rem;\n        border-top: 1px solid #D9D9D9;\n        border-bottom: 1px solid #D9D9D9;\n        display: flex;\n        flex-direction: column;\n        gap: 2rem;\n    }\n\n    .settings_modal-main-items {\n        display: flex;\n        flex-direction: column;\n        gap: 2rem;\n    }\n\n    .settings_modal-main-item {\n        display: flex;\n        flex-direction: column;\n        gap: 1.5rem;\n    }\n\n    .settings_modal-main-item_variants {\n        display: flex;\n        flex-direction: column;\n        gap: 1rem;\n    }\n\n    .settings_modal-main-item_title {\n        font-size: 16px;\n    }\n\n    .choice_radio-container {\n        display: flex;\n        gap: 1rem;\n    }\n\n    .choice_radio-container label {\n        margin-bottom: 0;\n    }\n\n    .settings_modal-footer {\n        padding: 0 2rem;\n        padding-top: 1rem;\n        display: flex;\n        gap: 1rem;\n        align-items: center;\n        justify-content: flex-start;\n    }\n\n    .settings_modal-button {\n        padding: .5rem;\n        border-radius: 4px;\n        border: 1px solid #0055FF;\n    }\n\n    .settings_modal-button.confirm {\n        color: white;\n        background-color: #0055FF;\n    }\n\n    .settings_modal-button.cancel {\n        color: #0055FF;\n        background-color: white;\n    }\n    \n\n\n    .qr-modal {\n        position: absolute;\n        width: 100%;\n        height: 100%;\n        background-color: #00000033;\n        z-index: 100;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        transition: all .3s;\n        opacity: 1;\n    }\n\n    .qr-modal.hidden {\n        opacity: 0;\n        z-index: -1;\n    }\n\n    .dropdown-modal {\n        position: absolute;\n        width: 100%;\n        height: 100%;\n        z-index: 9;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        transition: all .3s;\n        opacity: 1;\n    }\n\n    .dropdown-modal.hidden {\n        opacity: 0;\n        z-index: -1;\n    }\n\n    .popover-modal {\n        position: absolute;\n        width: 100%;\n        height: 100%;\n        z-index: 99;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        transition: all .3s;\n        opacity: 1;\n    }\n\n    .popover-modal.hidden {\n        opacity: 0;\n        z-index: -1;\n    }\n\n    .app-qr-info-container {\n        position: absolute;\n        width: 100vw;\n        height: 100vh;\n        z-index: 99;\n        opacity: 1;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        background-color: #acacac5e;\n    }\n\n    .app-qr-info {\n        display: flex;\n        background-color: white;\n        z-index: 99;\n        opacity: 1;\n        transition: all 0.3s;\n        max-width: 35%;\n        gap: 1rem;\n        padding: 2rem;\n        border-radius: 10px;\n    }\n\n    .app-qr-info-container.hidden {\n        z-index: -1;\n        opacity: 0;\n        transition: all 0.3s;\n    }\n\n    .app-qr-info.hidden {\n        z-index: -1;\n        opacity: 0;\n    }\n\n    .qr-code-info {\n        display: flex;\n        flex-direction: column;\n        flex-shrink: 2;\n    }\n\n    .app-store-info {\n        display: flex;\n        gap: 1rem\n    }\n\n    .store-image {\n        width: 11rem;\n        height: 5rem;\n    }\n\n    p {\n        margin-top: 0;\n        margin-bottom: 0;\n        padding-top: 0;\n        padding-bottom: 0;\n    }\n\n    h3 {\n        font-size: 2rem;\n    }\n\n    svg {\n        fill: none;\n    }\n\n    .content-body__inner {\n        padding: 0;\n    }\n\n    .portal-container {\n        width: 100%;\n        height: 100%;\n    }\n\n    .portal-content {\n        display: flex;\n        /* 70px - высота шапки */\n        padding-top: 70px;\n    }\n\n    .main-page {\n        display: flex;\n        flex: 2 1 30%;\n        width: 100%;\n        position: relative;\n        flex-direction: column;\n        background-color: #F5F6FA;\n        min-height: 100dvh;\n    }\n\n    .main-page_main-content {\n        background-color: #F5F6FA;\n        padding: 30px 20px 44px;\n        display: flex;\n        flex-direction: column;\n        overflow: hi