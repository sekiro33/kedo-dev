msgctxt ".descriptor.template.content.[4].values.html"
msgid "\u003cstyle\u003e\n    .goskey-info-container {\n        display: flex;\n        flex-direction: column;\n        gap: 1rem;\n    }\n    .mobile-type {\n        display: none;\n    }\n    elma-groupbox {\n        border: none !important;\n        padding: 0 !important;\n        margin-bottom: 1rem;\n        display: flex !important;\n        flex-direction: column;\n        gap: 1rem;\n    }\n    .groupbox-header {\n        margin-bottom: 0 !important;\n        position: relative !important;\n        width: fit-content !important;\n    }\n\n    .groupbox-header button {\n        position: absolute;\n        right: -3rem;\n        top: 2px;\n    }\n\n    .mobile-type .groupbox-header {\n        position: relative;\n    }\n    .mobile-type .groupbox-header button {\n        position: absolute;\n        right: -2rem;\n        top: 0;\n    }\n    .step-label {\n        font-weight: 600\n    }\n    .goskey-step {\n        display: flex;\n        flex-direction: column;\n        margin-bottom: 2rem;\n    }\n    .step-images {\n        display: flex;\n        flex-wrap: wrap;\n    }\n    .step-img {\n        width: 100%;\n    }\n    .svg-link {\n        max-width: 150px;\n        max-height: 150px;\n    }\n    .svg-link g {\n        fill: black;\n    }\n    .qr-link-container {\n        display: flex;\n        justify-content: flex-start;\n        gap: 2rem;\n        flex-wrap: wrap;\n    }\n\n    @media (max-width: 1000px) {\n        .goskey-step {\n            display: none;\n        }\n        .mobile-type {\n            display: block;\n        }\n    }\n\n    @media (max-width: 600px) {\n        .svg-link {\n            display: none;\n        }\n        .qr-link-container {\n            display: flex;\n            flex-wrap: nowrap;\n        }\n    }\n\u003c/style\u003e"
msgstr "\u003cstyle\u003e\n    .goskey-info-container {\n        display: flex;\n        flex-direction: column;\n        gap: 1rem;\n    }\n    .mobile-type {\n        display: none;\n    }\n    elma-groupbox {\n        border: none !important;\n        padding: 0 !important;\n        margin-bottom: 1rem;\n        display: flex !important;\n        flex-direction: column;\n        gap: 1rem;\n    }\n    .groupbox-header {\n        margin-bottom: 0 !important;\n        position: relative !important;\n        width: fit-content !important;\n    }\n\n    .groupbox-header button {\n        position: absolute;\n        right: -3rem;\n        top: 2px;\n    }\n\n    .mobile-type .groupbox-header {\n        position: relative;\n    }\n    .mobile-type .groupbox-header button {\n        position: absolute;\n        right: -2rem;\n        top: 0;\n    }\n    .step-label {\n        font-weight: 600\n    }\n    .goskey-step {\n        display: flex;\n        flex-direction: column;\n        margin-bottom: 2rem;\n    }\n    .step-images {\n        display: flex;\n        flex-wrap: wrap;\n    }\n    .step-img {\n        width: 100%;\n    }\n    .svg-link {\n        max-width: 150px;\n        max-height: 150px;\n    }\n    .svg-link g {\n        fill: black;\n    }\n    .qr-link-container {\n        display: flex;\n        justify-content: flex-start;\n        gap: 2rem;\n        flex-wrap: wrap;\n    }\n\n    @media (max-width: 1000px) {\n        .goskey-step {\n            display: none;\n        }\n        .mobile-type {\n            display: block;\n        }\n    }\n\n    @media (max-width: 600px) {\n        .svg-link {\n            display: none;\n        }\n        .qr-link-container {\n            display: flex;\n            flex-wrap: nowrap;\n        }\n    }\n\u003c/style\u003e"
