msgid "\u003cscript\u003e\n    document.addEventListener(\"DOMContentLoaded\", () =\u003e {\n        const form = document.querySelector('elma-form');\n        if (form) {\n            const buttons = [...form.querySelectorAll('buttons')];\n            console.log(buttons)\n            const zupButton = buttons.find(f =\u003e f.innerText == 'Доотправить документы');\n            const attachFileFlag = \u003c%-Context%\u003e.data.attach_files;\n            if (zupButton) {\n                zupButton.disabled = attachFileFlag;\n            }\n        }\n    })\n\u003c/script\u003e"
msgstr "\u003cscript\u003e\n    document.addEventListener(\"DOMContentLoaded\", () =\u003e {\n        const form = document.querySelector('elma-form');\n        if (form) {\n            const buttons = [...form.querySelectorAll('buttons')];\n            console.log(buttons)\n            const zupButton = buttons.find(f =\u003e f.innerText == 'Доотправить документы');\n            const attachFileFlag = \u003c%-Context%\u003e.data.attach_files;\n            if (zupButton) {\n                zupButton.disabled = attachFileFlag;\n            }\n        }\n    })\n\u003c/script\u003e"

